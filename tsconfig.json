{
  "compilerOptions": {
    "target": "ES5",
    "module": "ES6",
    "moduleResolution": "node",
    "jsx": "react-jsxdev",
    "strict": true,
    "rootDir": "./",
    "baseUrl": "./src",
    "paths": {
      "typings/*": ["typings/*"],
      "utils/*": ["utils/*"],
    },
    "allowSyntheticDefaultImports": true
  },
  "include": ["./src"],
}